    <script>
      function _log(text) {
        var logtxt = document.getElementById('log');
        logtxt.value = text;
      }
    </script>

    <vbox>
      <label control="your-name" value="Ontology URL:"/>
      <hbox>
        <textbox id="ontology-url" value=""/>
        <button label="Load" oncommand="selectowl.load();"/>
        <button label="Refresh" oncommand="selectowl.refreshList();"/>
      </hbox>
      <textbox id="jscon" value="" onkeypress="if(event.keyCode == 13)_log(eval(event.target.value));"/>
      <textbox id="log" value="" multiline="true" height="260" style="font-size: 10px; font-family: courier; "/>
      <listbox id="ontology-resources-list"></listbox>
    </vbox>



