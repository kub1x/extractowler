    <hbox id="selectowl-breadcrumbs" hidden="true">
        <textbox id="selectowl-path" flex="1" readonly="true" />
        <button label="&selectowl.breadcrumbs.back;" oncommand="selectowl.gui.urlBack();" />
    </hbox>

    <tree id="selectowl-tree" flex="1" hidecolumnpicker="false" seltype="single" class="tree" rows="5" onselect="selectowl.gui.onSelect();" onclick="selectowl.gui.onClick(event);">
        <treecols id="selectowl-tree-cols">
            <treecol id="selectowl-tree-col-name" primary="true" flex="0" label="name" persist="width" ordinal="1"/>

            <splitter class="tree-splitter" ordinal="2"/>

            <treecol id="selectowl-tree-col-area" flex="1" label="area" ordinal="3" />

            <splitter class="tree-splitter" ordinal="4"/>

            <treecol id="selectowl-tree-col-url" label="url" ordinal="5" hidden="true"/>

            <splitter class="tree-splitter" ordinal="6"/>

            <treecol id="selectowl-tree-col-read" label="read" ordinal="7" hidden="true"/>

            <splitter class="tree-splitter" ordinal="8"/>

            <treecol id="selectowl-tree-col-use" label="use" ordinal="9" hidden="true"/>

            <splitter class="tree-splitter" ordinal="10"/>

            <treecol id="selectowl-tree-col-attach" label="attach" ordinal="11" hidden="true"/>
